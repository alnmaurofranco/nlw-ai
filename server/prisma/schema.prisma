generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Video {
  id            String   @id @default(uuid())
  name          String
  path          String
  transcription String?
  createdAt     DateTime @default(now()) @map("created_at")

  @@map("videos")
}

model Prompt {
  id       String @id @default(uuid())
  title    String
  template String

  @@map("prompts")
}
